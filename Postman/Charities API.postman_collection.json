{
	"info": {
		"_postman_id": "bd70cc8e-a583-4ef1-8d91-9528bd64a083",
		"name": "Charities API",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "AT Get Charity Details",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.environment.set(\"charityRegistrationNumber\", \"200055\");\r",
							"pm.environment.set(\"bearerToken\", \"\");"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "X-Version",
						"value": "1",
						"type": "text"
					},
					{
						"key": "Authorization",
						"value": "Bearer {{bearerToken}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "https://at-charities-api.apprenticeships.education.gov.uk/api/Charities/{{charityRegistrationNumber}}",
					"protocol": "https",
					"host": [
						"at-charities-api",
						"apprenticeships",
						"education",
						"gov",
						"uk"
					],
					"path": [
						"api",
						"Charities",
						"{{charityRegistrationNumber}}"
					]
				}
			},
			"response": []
		}
	]
}